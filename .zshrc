  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

# Customize to your needs...
eval `ssh-agent` > /dev/null 2>&1
eval `ssh-add ~/.ssh/id_ed25519 > /dev/null 2>&1`
